{
  "presets": [
    ["env", { "modules": false }],
    "stage-2",
    "es2015"
  ],
  "plugins": [
    "transform-runtime",
    "transform-remove-strict-mode",
    ["component", [
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ]]

  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul","transform-remove-strict-mode"]
    }
  }
}
